package thread.basic;


public class ThreadPriorites1 {

	public static void main(String[] args) {
		
		MyTHread t = new MyTHread();
		t.setPriority(9);
		t.start();
		
		for(int i = 1 ; i<=10 ; i++)
			System.out.println("Parent thread");
	}
}

class MyTHread extends Thread{
	
	public void run() {
		for(int i = 1 ; i<=10 ; i++)
			System.out.println("Child thread");
	}
}